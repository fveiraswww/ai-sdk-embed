{
  "name": "ai-sdk-memory",
  "version": "0.0.3",
  "description": "Semantic and intent-based memory for AI SDK. Supports one-shot semantic caching and multi-turn intent-aware conversations. Reduces token costs by reusing similar responses.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "bun run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "vercel",
    "cache",
    "semantic",
    "semantic-cache",
    "vector",
    "upstash",
    "redis",
    "embedding",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-sdk-memory.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-sdk-memory/issues"
  },
  "homepage": "https://github.com/yourusername/ai-sdk-memory#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.7.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ai": "^5.0.0"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@upstash/redis": "^1.35.5",
    "@upstash/vector": "^1.2.2",
    "dotenv": "^17.2.3",
    "zod": "4.1.12"
  }
}
